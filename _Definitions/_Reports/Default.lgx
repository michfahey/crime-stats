<?xml version="1.0" encoding="utf-8"?>
<Report
	ID="Default"
	idePositioning="Absolute"
	>
	<DefaultRequestParams
		inpState="All"
		inpType="dlNonViolentCrimeData"
		inpYear="2010"
		paramNonViolent="Bar"
		paramViolent="Bar"
	/>
	<LocalData
		ID="localCrimeData"
		>
		<DataLayer
			CellRange="A2:O105"
			ExcelFile="@Function.AppPhysicalPath~\_SupportFiles\CrimeData.xls"
			ExcelWorksheet="combined"
			ID="ViolentCrimeData"
			Type="Excel"
			>
			<DataLayerLink
				ID="dlViolentCrimeData"
			/>
		</DataLayer>
		<DataLayer
			CellRange="A2:O105"
			ExcelFile="@Function.AppPhysicalPath~\_SupportFiles\nonViolentCrimes.xls"
			ExcelWorksheet="combined"
			ID="nonViolentCrimeData"
			Type="Excel"
			>
			<DataLayerLink
				ID="dlNonViolentCrimeData"
			/>
		</DataLayer>
	</LocalData>
	<LocalData
		ID="localRange"
		>
		<DataLayer
			ID="dlRange"
			Type="Static"
			>
			<StaticDataRow
				medStart="=iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 2799.99, 400.01)"
			/>
			<CalculatedColumn
				Formula="iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 5000, 2000)"
				ID="highEnd"
			/>
			<CalculatedColumn
				Formula="iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 3500, 600.01)"
				ID="highStart"
			/>
			<CalculatedColumn
				Formula="iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 2600, 400)"
				ID="lowEnd"
			/>
			<CalculatedColumn
				Formula="iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 3499.99, 600)"
				ID="medEnd"
			/>
			<CalculatedColumn
				Formula="iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, 2599.99, 400.01)"
				ID="medStart"
			/>
		</DataLayer>
	</LocalData>
	<LocalData
		ID="localChartScale"
		>
		<DataLayer
			CellRange="A1:C19"
			ColumnNamesRow="1"
			ExcelFile="@Function.AppPhysicalPath~\_SupportFiles\popDetails.xls"
			ExcelWorksheet="@Request.inpState~"
			ID="ExcelDataLayer"
			Type="Excel"
			>
			<AggregateColumn
				AggregateColumn="Male"
				AggregateFunction="Max"
				ID="maxMale"
			/>
			<CalculatedColumn
				Formula="@Data.maxMale~ + @Data.maxMale~ * .25"
				ID="chtScale"
			/>
			<DataLayerLink
				ID="lnkPop"
			/>
		</DataLayer>
	</LocalData>
	<Body>
		<Division
			Class="center-block"
			HtmlDiv="True"
			ID="divCenterBlock"
			>
			<Rows
				Class="divCenter"
				ID="rowsContent"
				Width="800"
				WidthScale="px"
				>
				<Row
					ID="rowHeader"
					>
					<Column
						Class="backheader"
						ColSpan="3"
						ID="colHeader"
						Width="100"
						WidthScale="%"
						>
						<Division
							Class="imgizq"
							HtmlDiv="True"
							ID="divFingerPrint"
							>
							<Image
								Caption="Fingerprint.png"
								ID="imgFingerPrint"
								Width="70px"
							/>
						</Division>
						<Division
							HtmlDiv="True"
							ID="divTitles"
							>
							<Label
								Caption="Crime Statistics for the United States"
								Class="ThemeHeaderLarger"
								ID="lblHeader"
							/>
							<LineBreak/>
							<Label
								Caption="by State in 2009 and 2010"
								Class="ThemeHeaderLarge"
								ID="lblHeader2"
							/>
						</Division>
						<Division
							Class="imgder"
							HtmlDiv="True"
							ID="divLogo"
							>
							<Image
								Caption="LogiLogo.png"
								ID="imgLogiLogo"
							/>
						</Division>
					</Column>
				</Row>
				<Row
					Class="backselects"
					ID="rowUI"
					>
					<Column
						Class="ThemeAlignBottom"
						ColSpan="3"
						ID="colUI"
						>
						<Division
							ID="divUI"
							>
							<Spaces
								Size="2"
							/>
							<Image
								Caption="Gear.png"
								Class="ThemeAlignTop"
								ID="imgGear"
							/>
							<Spaces
								Size="2"
							/>
							<InputSelectList
								DefaultValue="@Request.inpType~"
								ID="inpType"
								OptionCaptionColumn="Type"
								OptionValueColumn="dataSource"
								>
								<DataLayer
									ID="dlSelectOptions"
									Type="Static"
									>
									<StaticDataRow
										dataSource="dlViolentCrimeData"
										Type="Violent Crimes"
										Year="2009"
									/>
									<StaticDataRow
										dataSource="dlNonViolentCrimeData"
										Type="Non-violent Crimes"
										Year="2010"
									/>
									<DataLayerLink
										ID="dlsSelectOptions"
									/>
								</DataLayer>
								<EventHandler
									DhtmlEvent="onchange"
									ID="evhOnChange"
									>
									<Action
										ElementID="rowsContent"
										ID="actRefreshTable1"
										Type="RefreshElement"
									/>
								</EventHandler>
							</InputSelectList>
							<Spaces
								Size="2"
							/>
							<Image
								Caption="Date.png"
								Class="ThemeAlignTop"
								ID="imgCalendar"
							/>
							<Spaces
								Size="2"
							/>
							<InputSelectList
								DefaultValue="@Request.inpYear~"
								ID="inpYear"
								OptionCaptionColumn="Year"
								OptionValueColumn="Year"
								>
								<DataLayer
									ID="dlInputList"
									LinkedDataLayerID="dlsSelectOptions"
									Type="Linked"
								/>
								<EventHandler
									DhtmlEvent="onchange"
									ID="evhOnChange"
									>
									<Action
										ElementID="rowsContent"
										ID="actRefreshTable2"
										Type="RefreshElement"
									/>
								</EventHandler>
							</InputSelectList>
							<Spaces
								Size="2"
							/>
							<Image
								Caption="State.png"
								Class="ThemeAlignTop"
								ID="imgMarker"
							/>
							<Spaces
								Size="2"
							/>
							<InputSelectList
								DefaultValue="@Request.inpState~"
								ID="inpState"
								IncludeBlank="True"
								IncludeBlankCaption="All States"
								IncludeBlankValue="All"
								OptionCaptionColumn="Column3"
								OptionValueColumn="Column3"
								>
								<DataLayer
									ID="dlStates"
									LinkedDataLayerID="dlNonViolentCrimeData"
									Type="Linked"
									>
									<ConditionFilter
										Condition="@Request.inpYear~ == @Data.Column4~"
									/>
									<ConditionFilter
										Condition="&quot;All&quot; &lt;&gt; &quot;@Data.Column3~&quot;"
									/>
									<SortFilter
										DataType="Text"
										SortColumn="Column3"
									/>
								</DataLayer>
								<EventHandler
									DhtmlEvent="onchange"
									ID="evhOnChange"
									>
									<Action
										ElementID="colLeft, colRight"
										ID="actRefreshCharts"
										Type="RefreshElement"
									/>
								</EventHandler>
							</InputSelectList>
						</Division>
					</Column>
				</Row>
				<Row
					Class="backcontent"
					ID="rowVisuals"
					>
					<Column
						Class="ThemeAlignTop"
						ID="colLeft"
						Width="280px"
						>
						<Division
							Class="ThemeAlignCenter"
							HtmlDiv="True"
							ID="divList"
							>
							<DataTable
								AjaxPaging="True"
								AltRowClass="dtBullet"
								Class="ThemeAlignTop"
								ID="dtNavList"
								RowClass="dtBullet"
								SortArrows="True"
								Width="270"
								WidthScale="px"
								>
								<DataLayer
									ID="dlNavList"
									LinkedDataLayerID="@Request.inpType~"
									Type="Linked"
									>
									<SortFilter
										SortColumn="Column3"
									/>
									<ConditionFilter
										Condition="@Data.Column4~ == 2009"
									/>
									<ConditionFilter
										Condition="&quot;@Data.Column3~&quot; &lt;&gt; &quot;All&quot;"
									/>
									<Join
										ID="joinInner"
										JoinType="InnerJoin"
										>
										<MatchCondition
											ID="matchCol3"
											LeftDataColumn="Column3"
											RightDataColumn="Column3"
										/>
										<DataLayer
											ID="dlNavList"
											LinkedDataLayerID="@Request.inpType~"
											Type="Linked"
											>
											<SortFilter
												SortColumn="Column3"
											/>
											<ConditionFilter
												Condition="@Data.Column4~ == 2010"
											/>
											<CalculatedColumn
												Formula="@Data.Column15~"
												ID="total2010"
											/>
										</DataLayer>
									</Join>
									<CalculatedColumn
										Formula="@Data.Column15~ - @Data.total2010~"
										ID="difference"
									/>
								</DataLayer>
								<DataTableColumn
									Header="State"
									ID="colState"
									Width="130"
									WidthScale="px"
									>
									<DataColumnSort
										DataColumn="State"
									/>
									<Rows
										Width="100"
										WidthScale="%"
										>
										<Row>
											<Column
												Class="border0"
												ID="colStateName"
												Width="80"
												WidthScale="%"
												>
												<Label
													Caption="@Data.Column3~"
													ID="lblState"
												/>
											</Column>
											<Column
												Class="ThemeAlignRight"
												ID="colGauge"
												Width="20"
												WidthScale="%"
												>
												<Gauge
													ChartHeight="10"
													ChartWidth="10"
													ID="gaugeComparison"
													Type="Indicator"
													Value="@Data.difference~"
													>
													<IndicatorState
														Color="Red"
														ID="trendUp"
														Image="Up_Arrow.png"
														IndicatorGaugeStyle="Image"
														IndicatorLowValue="0"
													/>
													<IndicatorState
														Color="Green"
														ID="trendDown"
														Image="Down_Arrow.png"
														IndicatorGaugeStyle="Image"
														IndicatorHighValue="0"
													/>
												</Gauge>
											</Column>
										</Row>
									</Rows>
								</DataTableColumn>
								<DataTableColumn
									Class="noPadding"
									Header="=iif(&quot;@Request.inpType~&quot; == &quot;dlNonViolentCrimeData&quot;, &quot;Non-Violent Crimes (@Request.inpYear~ vs U.S. avg)&quot;, &quot;Violent Crimes &amp;nbsp;&amp;nbsp;&amp;nbsp;(@Request.inpYear~ vs U.S. avg)&quot;)"
									HeaderType="Text"
									ID="colChart"
									Width="140"
									WidthScale="px"
									>
									<IncludeFrame
										FrameBorder="False"
										ID="srBulletChart"
										SubReportMode="Embedded"
										>
										<LinkParams
											paramState="@Data.Column3~"
											paramType="@Request.inpType~"
											paramYear="@Data.Column4~"
										/>
										<Target
											ID="tgtBulletCharts"
											LinkDataLayers="True"
											Report="sr_Bullets"
											Type="IncludeFrameReport"
										/>
									</IncludeFrame>
								</DataTableColumn>
								<InteractivePaging
									CaptionType="Image"
									Class="colpages"
									HideWhenOnePage="True"
									Location="Bottom"
									PageRowCount="12"
									ShowPageNumber="True"
								/>
							</DataTable>
						</Division>
					</Column>
					<Column
						Class="ThemeAlignTop"
						ID="colRight"
						Width="520"
						>
						<Rows
							Class="ThemeAlignCenter"
							ID="rowsContainer"
							Width="520"
							WidthScale="px"
							>
							<Row
								Class="violentback"
								ID="rowBubble"
								>
								<Column
									ID="colBubble"
									>
									<Rows
										TableBorder="0"
										>
										<Row
											ID="rowHeaderBubble"
											>
											<Column
												Class="ThemeAlignCenter, ThemeHeaderLarger"
												ColSpan="2"
												ID="colHeaderBubble"
												>
												<Note
													Note="Note trailing space in Label captions"
												/>
												<Label
													Caption="Violent Crime "
													ID="lblHeaderBubble1"
												/>
												<Label
													Caption="by "
													Class="fontLtGray "
													ID="lblHeaderBubble2"
												/>
												<Label
													Caption="Non-Violent Crime "
													ID="lblHeaderBubble3"
												/>
												<Label
													Caption="per "
													Class="fontLtGray"
													ID="lblHeaderBubble4"
												/>
												<Label
													Caption="100,000"
													ID="lblHeaderBubble5"
												/>
											</Column>
										</Row>
										<Row>
											<Column
												ID="colChartBubble"
												>
												<ChartCanvas
													ChartHeight="250"
													ChartWidth="580"
													>
													<DataLayer
														ID="dlViolent"
														LinkedDataLayerID="dlViolentCrimeData"
														Type="Linked"
														>
														<ConditionFilter
															Condition="@Data.Column4~ ==@Request.inpYear~"
															ID="FilterYear"
														/>
														<RankColumn
															DataColumn="Column5"
															ID="Rank"
															RankType="Rank"
														/>
														<CalculatedColumn
															Formula="(@Data.Rank~/50 )*20  + 2"
															ID="FinalSize"
														/>
														<ConditionFilter
															Condition="&quot;@Data.Column3~&quot; == &quot;@Request.inpState~&quot;"
															ID="filterSelectedState"
														/>
														<RemoveColumns
															ColumnNames="Column1, Column15, FinalSize, Column3, Rank"
															ID="keepNeeded"
															RemoveColumnsType="Keep"
														/>
														<Join
															ID="violentAndNonViolent"
															JoinType="InnerJoin"
															>
															<MatchCondition
																DataType="Text"
																LeftDataColumn="Column3"
																RightDataColumn="Column3"
															/>
															<DataLayer
																ID="dlNonViolent"
																LinkedDataLayerID="dlNonViolentCrimeData"
																Type="Linked"
																>
																<CalculatedColumn
																	Formula="@Data.Column15~"
																	ID="TotalNonViolent"
																/>
