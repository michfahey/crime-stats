														</Quicktip>
													</Series>
													<Series
														BubbleMaxSize="30"
														ChartSizeDataColumn="200"
														ChartXDataColumn="Column15"
														ChartYDataColumn="TotalNonViolent"
														Color="#DCD440"
														ID="esIMidwest"
