																	ID="matchState"
																	LeftDataColumn="Column3"
																	RightDataColumn="Column3"
																/>
																<DataLayer
																	ID="dlNonViolent"
																	LinkedDataLayerID="dlNonViolentCrimeData"
																	Type="Linked"
																	>
																	<ConditionFilter
																		Condition="@Data.Column4~ ==@Request.inpYear~"
																		ID="FilterYear"
																	/>
																	<CalculatedColumn
																		Formula="@Data.Column15~"
																		ID="TotalNonViolent"
																	/>
																	<RemoveColumns
																		ColumnNames="Column3, Column15, TotalNonViolent"
																		ID="StateAndvalue"
																		RemoveColumnsType="Keep"
																	/>
																</DataLayer>
															</Join>
														</DataLayer>
